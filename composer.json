{
    "name": "peej/contentacle",
    "description": "The Git and Markdown powered content management system",
    "keywords": ["cms", "markdown"],
    "homepage": "http://peej.github.com/contentacle",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul James",
            "email": "paul@peej.co.uk"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "pimple/pimple": "2.*",
        "smarty/smarty": "3.*",
        "peej/tonic": "3.*",
        "peej/git": "dev-master",
        "michelf/php-markdown": "1.*"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev",
        "behat/behat": "2.*",
        "behat/mink": "1.*",
        "behat/mink-extension": "1.*",
        "behat/mink-goutte-driver": "1.*"
    },
    "autoload": {
        "psr-0": {
            "Contentacle": "src/"
        }
    }
}
